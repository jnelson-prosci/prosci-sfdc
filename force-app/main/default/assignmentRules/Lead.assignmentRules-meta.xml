        <ruleEntry>
            <assignedTo>Europe_Leads_Nordic_Region</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>AND(
				Queue_Family__c = &apos;Europe&apos;,
				OR(
        CONTAINS(Languages__c, &apos;Danish&apos;),
				    CONTAINS(Languages__c, &apos;Norwegian&apos;),
				    CONTAINS(Languages__c, &apos;Icelandic&apos;),
				    CONTAINS(Languages__c, &apos;Finnish&apos;),
				    CONTAINS(Languages__c, &apos;Greenlandic&apos;)
    )
)</formula>
        </ruleEntry>
