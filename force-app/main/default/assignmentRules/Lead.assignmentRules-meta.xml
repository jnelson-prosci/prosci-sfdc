<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>ANZ Lead Assignments</fullName>
        <active>false</active>
        <ruleEntry>
            <assignedTo>ANZ_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>OR(ISPICKVAL(Country_Code__c, &apos;AU&apos;),ISPICKVAL(Country_Code__c, &apos;NZ&apos;))</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>gsingh@prosci.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Interest__c</field>
                <operation>contains</operation>
                <value>CMP</value>
            </criteriaItems>
            <template>X001_All_auto_response_emails_WebtoLead/LeadsNewassignmentnotificationSAMPLE</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>gsingh@prosci.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Interest__c</field>
                <operation>contains</operation>
                <value>bookstore</value>
            </criteriaItems>
            <template>X001_All_auto_response_emails_WebtoLead/X07_AUTO_response_to_Bookstore_order_from_website</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>gsingh@prosci.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Interest__c</field>
                <operation>equals</operation>
                <value>Contact,Change Community</value>
            </criteriaItems>
            <template>X001_All_auto_response_emails_WebtoLead/X04_Auto_Response_for_Prosci_CMP_Public_Website_Registrations</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>gsingh@prosci.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Interest__c</field>
                <operation>equals</operation>
                <value>Webinar ANZ</value>
            </criteriaItems>
            <template>unfiled$public/LeadsWebtoLeademailresponseSAMPLE</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>gsingh@prosci.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Interest__c</field>
                <operation>contains</operation>
                <value>info,Contact us</value>
            </criteriaItems>
            <template>X001_All_auto_response_emails_WebtoLead/X01_Auto_response_Contact_Use_form_on_websitefor_info_form</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>gsingh@prosci.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Interest__c</field>
                <operation>equals</operation>
                <value>MGMT - ANZ</value>
            </criteriaItems>
            <template>X001_All_auto_response_emails_WebtoLead/LeadsNewassignmentnotificationSAMPLE</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>gsingh@prosci.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Interest__c</field>
                <operation>contains</operation>
                <value>Agile</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
