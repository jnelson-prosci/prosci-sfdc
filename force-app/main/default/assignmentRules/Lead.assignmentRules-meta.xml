        <ruleEntry>
            <assignedTo>US_Leads_Carnivores</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>AND(                 ISPICKVAL(Country_Code__c, &apos;US&apos;),                 OR(                     ISPICKVAL(Industry, &apos;Finance/Banking&apos;),                     ISPICKVAL(Industry, &apos;Insurance&apos;),                     ISPICKVAL(Industry, &apos;Consulting&apos;),                     ISPICKVAL(Industry, &apos;Professional, Scientific, &amp; Tech Service&apos;),                     ISPICKVAL(Industry, &apos;Transportation &amp; Warehousing&apos;)                 )             )</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>US_Leads_Nitro</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>AND(                 ISPICKVAL(Country_Code__c, &apos;US&apos;),                 OR(                     ISPICKVAL(Industry, &apos;Non-profit&apos;),                     CONTAINS(TEXT(Industry), &apos;Government&apos;),                     ISPICKVAL(Industry, &apos;Pharmaceutical&apos;),                     ISPICKVAL(Industry, &apos;Education Services&apos;)                 )             )</formula>
        </ruleEntry>
